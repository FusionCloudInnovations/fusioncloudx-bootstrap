# See .env.secrets for OP_SERVICE_ACCOUNT_TOKEN
#
# FusionCloudX Bootstrap .env
# All configuration values for bootstrap.ps1 are centralized here.
# Example values provided; update as needed for your environment.

# WSL distro names
WSL_DISTRO_NAME=Ubuntu
WSL_CUSTOM_DISTRO_NAME=Ubuntu-FCX

# Git config
GIT_USER_NAME=Your Name
GIT_USER_EMAIL=your@email.com

# Windows Update (true/false)
WINDOWS_UPDATE_NOW=true

# Ephemeral mode (true/false)
EPHEMERAL_MODE=true
CLEAN_RUN=true